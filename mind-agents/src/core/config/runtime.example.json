{
  "tickInterval": 1000,
  "maxAgents": 10,
  "logLevel": "info",
  "persistence": {
    "enabled": true,
    "path": "./data"
  },
  "extensions": {
    "autoLoad": true,
    "paths": ["./extensions"]
  },
  "portals": {
    "autoLoad": true,
    "paths": ["./portals"],
    "apiKeys": {
      "openai": "YOUR_OPENAI_API_KEY",
      "anthropic": "YOUR_ANTHROPIC_API_KEY",
      "groq": "YOUR_GROQ_API_KEY",
      "xai": "YOUR_XAI_API_KEY",
      "openrouter": "YOUR_OPENROUTER_API_KEY",
      "kluster.ai": "YOUR_KLUSTERAI_API_KEY"
    },
    "modelConfig": {
      "openai": {
        "model": "gpt-4.1-mini",
        "chatModel": "gpt-4.1-mini",
        "toolModel": "gpt-4.1-mini",
        "embeddingModel": "text-embedding-3-large",
        "imageModel": "dall-e-3"
      }
    }
  }
}
