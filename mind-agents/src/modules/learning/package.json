{
  "name": "@symindx/learning-system",
  "version": "1.0.0",
  "description": "Advanced learning system for SYMindX agents with continual learning, few-shot adaptation, neural architecture search, reinforcement learning, and knowledge distillation",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "artificial-intelligence",
    "machine-learning",
    "continual-learning",
    "few-shot-learning",
    "neural-architecture-search",
    "reinforcement-learning",
    "knowledge-distillation",
    "meta-learning",
    "online-learning",
    "adaptive-systems",
    "agent-learning",
    "multi-agent-systems"
  ],
  "author": "SYMindX Development Team",
  "license": "MIT",
  "dependencies": {
    "events": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.test.ts",
      "!**/node_modules/**",
      "!**/examples/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/symindx/symindx.git",
    "directory": "mind-agents/src/modules/learning"
  },
  "bugs": {
    "url": "https://github.com/symindx/symindx/issues"
  },
  "homepage": "https://github.com/symindx/symindx#readme",
  "exports": {
    ".": {
      "import": "./index.ts",
      "require": "./index.ts"
    },
    "./types": {
      "import": "./types.ts",
      "require": "./types.ts"
    },
    "./online": {
      "import": "./online/index.ts",
      "require": "./online/index.ts"
    },
    "./adaptation": {
      "import": "./adaptation/index.ts",
      "require": "./adaptation/index.ts"
    },
    "./nas": {
      "import": "./nas/index.ts",
      "require": "./nas/index.ts"
    },
    "./rl": {
      "import": "./rl/index.ts",
      "require": "./rl/index.ts"
    },
    "./distillation": {
      "import": "./distillation/index.ts",
      "require": "./distillation/index.ts"
    }
  },
  "files": [
    "*.ts",
    "online/",
    "adaptation/",
    "nas/",
    "rl/",
    "distillation/",
    "examples/",
    "README.md"
  ]
}
