{
  "id": "example-agent",
  "name": "Example Agent",
  "description": "Example configuration showing granular model control",
  "version": "1.0.0",
  
  "personality": {
    "traits": {
      "creativity": 0.8,
      "analytical": 0.7,
      "empathy": 0.9
    },
    "backstory": "An AI agent demonstrating advanced model configuration",
    "goals": ["Learn", "Help", "Explore"],
    "values": ["Knowledge", "Collaboration", "Growth"]
  },
  
  "portals": [
    {
      "name": "primary-chat",
      "type": "openai",
      "enabled": true,
      "primary": true,
      "capabilities": ["chat_generation", "tool_calling"],
      "config": {
        "chatModel": "gpt-4o",
        "toolModel": "gpt-4o",
        "temperature": 0.7,
        "max_tokens": 2048
      }
    },
    {
      "name": "embeddings",
      "type": "openai",
      "enabled": true,
      "capabilities": ["embedding_generation"],
      "config": {
        "embeddingModel": "text-embedding-3-large",
        "embeddingDimensions": 3072
      }
    },
    {
      "name": "image-generation",
      "type": "openai",
      "enabled": true,
      "capabilities": ["image_generation"],
      "config": {
        "imageModel": "dall-e-3"
      }
    },
    {
      "name": "fast-inference",
      "type": "groq",
      "enabled": true,
      "capabilities": ["chat_generation"],
      "config": {
        "chatModel": "mixtral-8x7b-32768",
        "temperature": 0.5
      }
    },
    {
      "name": "local-llm",
      "type": "ollama",
      "enabled": false,
      "capabilities": ["chat_generation", "embedding_generation"],
      "config": {
        "chatModel": "llama3.1:70b",
        "embeddingModel": "nomic-embed-text",
        "baseUrl": "http://localhost:11434"
      }
    }
  ],
  
  "memory": {
    "type": "sqlite",
    "config": {
      "database_path": "./data/example_memories.db",
      "retention_policy": "30_days",
      "emotional_weighting": true,
      "enable_embeddings": true,
      "embedding_provider": "openai"
    }
  },
  
  "emotion": {
    "type": "rune_emotion_stack",
    "config": {
      "emotional_range": "full",
      "intensity_multiplier": 1.0,
      "emotional_memory": true
    }
  },
  
  "cognition": {
    "type": "hybrid",
    "config": {
      "planning_horizon": "medium",
      "creativity_boost": true,
      "analytical_depth": "deep"
    }
  }
}